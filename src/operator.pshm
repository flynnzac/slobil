  #length=1@
  ##CHECK_ARGS~$;

  #num=1@
  #type=Register@
  ##GETARG~$;

  registry* to_use = reg;
  if (a.length >= 3)
    {
      #num=2@
      #type=Registry@
      ##GETARG~$;

      to_use = (registry*) arg2->data;
    }

  data* obj = get(to_use, ((regstr*) arg1->data)->key, a.length < 3 ? 1 : 0);
  data* d;
  if (obj == NULL)
    {
      assign_boolean(&d, false);
    }
