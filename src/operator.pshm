void
op_get (arg a, registry* reg)
{
  #op=get@
  #length=1@
  ##CHECK_ARGS~$;

  #num=1@
  #type=Register@
  ##GETARG~$;
  
  registry* to_look;
  if (a.length == 2)
    {
      to_look = reg;
    }
  else
    {
      #num=2@
      #type=Registry@
      ##GETARG~$;

      to_look = (registry*) arg2->data;
    }

  data* ans = lookup(to_look, ((regstr*) arg1->data)->key,
		     a.length==2);
  if (ans != NULL)
    {
      ans = copy_data(ans);
      ret_ans(reg, ans);
    }
}

