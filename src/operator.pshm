void
op_interpreter (arg a, registry* reg)
{
  #op=interpreter@
  #length=0@
  ##CHECK_ARGS~$;

  data* d1 = NULL;
  data* d2 = NULL;

  for (int i=1; i < a.length; i = i + 2)
    {
      d1 = a.arg_array[i];
      if (d1->type != Register)
        {
          do_error("Expected a register.", reg->task->task);
          return;
        }

      d2 = resolve(a.arg_array[i+1], reg);
      d2 = copy_data(d2);
      set(wob_options, d2, ((regstr*) d1->data)->name, 1);
    }
}

  
